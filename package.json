{
	"name": "kesku-site",
	"version": "4.6.0",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && astro build",
		"build:quiet": "NODE_OPTIONS='--no-deprecation' astro check && NODE_OPTIONS='--no-deprecation' astro build",
		"preview": "astro preview",
		"format": "prettier -w --cache . && prettier -w --cache **/*.astro",
		"check": "astro check",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,scss,md,astro}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@astrojs/check": "^0.8.3",
