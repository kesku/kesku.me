		"astro": "4.12.1",
		"astro-expressive-code": "^0.35.3",
		"astro-icon": "^1.1.0",
		"cssnano": "^7.0.4",
		"rehype-external-links": "^3.0.0",
		"satori": "0.10.14",
		"satori-html": "^0.3.2",
		"sharp": "^0.33.4",
		"typescript": "^5.5.4"
	},
	"devDependencies": {
		"@astrojs/check": "^0.8.2",
		"@iconify-json/mdi": "^1.1.67",
