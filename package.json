		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-markdown": "^4.0.1",
		"eslint-plugin-regexp": "^2.5.0",
		"eslint-plugin-typescript-sort-keys": "^3.2.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.3.3",
